#include "iostream"
#include "math.h"

using namespace std;

double ConversorDeMoeda(double dol, double real, double conv);


int main() {

double dolar, rea, conver;
  
  conver = ConversorDeMoeda(dolar, rea, conver);

  
  cout << "Digite a cotação do dólar nos dias atuais no formato double: "<< endl;
  cin >> dolar;
  cout << "Digite o seu dinheiro: "<< endl;
  cin >> rea;
  cout << "Conversão: "<< conver << endl;

  return 0;
}

double ConversorDeMoeda(double dol, double real, double conv) {
  
   conv = dol*real;

  return 0; 
}